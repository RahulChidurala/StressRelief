<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hacklahoma.stressrelief.stressrelief">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/chill_app_launcher_round"
        android:label="ChillOut"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:largeHeap="true"
        android:hardwareAccelerated="false">
        <activity android:name=".UseCases.Menu.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UseCases.IndoorActivityMenu.IndoorActivity"></activity>
        <activity android:name=".UseCases.IndoorActivities.IndoorBreathing.IndoorBreathingActivity"></activity>
        <activity android:name=".UseCases.IndoorActivities.IndoorStretching.IndoorStrecthingActivity"></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>